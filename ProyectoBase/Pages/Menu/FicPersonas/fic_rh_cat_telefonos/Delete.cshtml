        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.UsuarioMod)
        </dd>
        <dt>
            Activo:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.Activo)
        </dd>
        <dt>
            Borrado:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.Borrado)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="rh_cat_telefono.IdTelefono" />
        <input type="submit" value="Borrar" class="btn btn-danger" /> |
        <a asp-page="./Index" class="btn btn-warning" asp-route-id="@Model.IdP" asp-route-control="@Model.Control" asp-route-searchString="@Model.SearchString" asp-route-ap="@Model.Ap" asp-route-am="@Model.Am">Regresar</a>
    </form>
</div>
