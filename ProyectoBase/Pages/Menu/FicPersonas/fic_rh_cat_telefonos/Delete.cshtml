@page
@model ProyectoBase.Pages.Menu.FicPersonas.fic_rh_cat_telefonos.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h2>Borrar</h2>

<h3>¿Esta seguro de querer eliminar este telefono?</h3>
<div>
    <h4>Informacion</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Codigo Pais:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.CodPais)
        </dd>
        <dt>
            No Telefono:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.NumTelefono)
        </dd>
        <dt>
            No Extensión:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.NumExtencion)
        </dd>
        <dt>
            Principal:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.Principal)
        </dd>
        <dt>
            Clave Referencia:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.ClaveReferencia)
        </dd>
        <dt>
            Referencia:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.Referencia)
        </dd>
        <dt>
            Fecha que registró:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.rh_cat_telefono.FechaReg)
