@page
@model ProyectoBase.Pages.Menu.FicPersonas.fic_seg_usuarios_estatus.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h2>Editar</h2>

<h4>Actualizar información</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="seg_usuarios_estatu.IdCtrlEstatus" />
            <div class="form-group">
                <label class="control-label">Id Usuario</label>
                <input asp-for="seg_usuarios_estatu.IdUsuario" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.IdUsuario" class="text-danger"></span>
            </div>



            <div class="form-group">
                <label class="control-label">Id Estatus</label>
                <select asp-items="ViewBag.IdEstatus" asp-for="seg_usuarios_estatu.IdEstatus" class="form-control"></select>
                <span asp-validation-for="seg_usuarios_estatu.IdEstatus" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label class="control-label">Id Tipo Estatus</label>
                <select asp-items="ViewBag.IdTipoEstatus" asp-for="seg_usuarios_estatu.IdTipoEstatus" class="form-control"></select>
                <span asp-validation-for="seg_usuarios_estatu.IdTipoEstatus" class="text-danger"></span>
            </div>




            <div class="form-group">
                <label class="control-label">Fecha Estatus</label>
                <input asp-for="seg_usuarios_estatu.FechaEstatus" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.FechaEstatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="seg_usuarios_estatu.Actual" class="control-label"></label>
                <input asp-for="seg_usuarios_estatu.Actual" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.Actual" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="seg_usuarios_estatu.Observacion" class="control-label"></label>
                <input asp-for="seg_usuarios_estatu.Observacion" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.Observacion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Fecha Registro</label>
                <input asp-for="seg_usuarios_estatu.FechaReg" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.FechaReg" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Ultima Modificación</label>
                <input asp-for="seg_usuarios_estatu.FechaUltMod" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.FechaUltMod" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Usuario que registro</label>
                <input asp-for="seg_usuarios_estatu.UsuarioReg" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.UsuarioReg" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Usuario que modifico</label>
                <input asp-for="seg_usuarios_estatu.UsuarioMod" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.UsuarioMod" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="seg_usuarios_estatu.Activo" class="control-label"></label>
                <input asp-for="seg_usuarios_estatu.Activo" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.Activo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="seg_usuarios_estatu.Borrado" class="control-label"></label>
                <input asp-for="seg_usuarios_estatu.Borrado" class="form-control" />
                <span asp-validation-for="seg_usuarios_estatu.Borrado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index" class="btn btn-warning">Atras</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
